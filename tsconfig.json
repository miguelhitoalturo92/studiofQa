{
  "compilerOptions": {
    "target": "es2017",
    "lib": ["dom", "es2017"],
    "module": "esnext",
    "moduleResolution": "node",
    "jsx": "react",
    "strict": false,
    "noEmit": true,
    "esModuleInterop": true,
    "skipLibCheck": true,
    "resolveJsonModule": true,
    "isolatedModules": true
  },
  "include": ["react/**/*"]
}
